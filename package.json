{
  "name": "pd3-vault-cracker",
  "private": true,
  "version": "0.1.3",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "dev:remote": "vite --host --open",
    "build": "pwa-assets-generator && vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tscon fig.json",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui --open",
    "test:watch": "vitest",
    "lint": "eslint -c .eslintrc --ext .svelte,.ts src/",
    "lint:styles": "stylelint --config .stylelintrc \"src/**/*.{css,scss}\"",
    "format": "eslint -c .eslintrc --fix --ext=.svelte,.ts --fix src",
    "format:prettier": "prettier --write src/**/{*.svelte,*.ts,*.scss}",
    "format:stylelint": "stylelint --config .stylelintrc \"src/**/*.{css,scss}\" --fix",
    "prepare": "husky install",
    "generate-pwa-assets": "pwa-assets-generator",
    "serve-json": "json-server --watch db.json --port 4000"
  },
  "devDependencies": {
    "@picocss/pico": "^1.5.10",
    "svelte": "^5.20.0",
    "@rollup/plugin-alias": "^5.0.1",
    "@sveltejs/kit": "^2.5.27",
    "@sveltejs/package": "^2.2.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/svelte": "^4.0.4",
    "@tsconfig/svelte": "^5.0.2",
    "@types/crypto-js": "^4.2.2",
    "@types/qrcode": "^1.5.5",
    "@types/sortablejs": "^1.15.8",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/adapter-static": "^3.0.6",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@sveltejs/vite-plugin-svelte-inspector": "^4.0.0",
    "@unocss/core": "^66.0.0",
    "@unocss/eslint-config": "^66.0.0",
    "@unocss/eslint-plugin": "^66.0.0",
    "@unocss/extractor-pug": "^66.0.0",
    "@unocss/extractor-svelte": "^66.0.0",
    "@unocss/reset": "^66.0.0",
    "@unocss/svelte-scoped": "^66.0.0",
    "@unocss/transformer-directives": "^66.0.0",
    "@unocss/transformer-variant-group": "^66.0.0",
    "@unocss/vite": "66.1.0-beta.3",
    "@vite-pwa/assets-generator": "^0.0.10",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.34.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-css-order": "^2.0.1",
    "prettier-plugin-svelte": "^3.0.3",
    "sass": "^1.69.3",
    "sass-loader": "^13.3.2",
    "svelte-check": "^3.5.2",
    "svelte-i18n": "^4.0.0",
    "svelte-preprocess": "^5.0.4",
    "sveltekit-autoimport": "^1.7.1",
    "tslib": "^2.6.2",
    "typed.js": "^2.0.12",
    "typescript": "^5.2.2",
    "unocss": "^0.57.1",
    "vite": "^6.0.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "^1.1.0",
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@types/cookie": "^0.5.3",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "cookie": "^0.5.0",
    "gsap": "3.8.0",
    "json-server": "^0.17.4",
    "vanilla-tilt": "^1.8.1"
  }
}
