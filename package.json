{
	"name": "pd3-vault-cracker",
	"type": "module",
	"version": "0.1.3",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"dev:remote": "vite --host --open",
		"build": "vite build",
    "start": "vite preview --port 3100",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tscon fig.json",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:ui": "vitest --ui --open",
		"test:watch": "vitest",
		"lint": "eslint -c .eslintrc --ext .svelte,.ts src/",
		"lint:styles": "stylelint --config .stylelintrc \"src/**/*.{css,scss}\"",
		"format": "eslint -c .eslintrc --fix --ext=.svelte,.ts --fix src",
		"format:prettier": "prettier --write src/**/{*.svelte,*.ts,*.scss}",
		"format:stylelint": "stylelint --config .stylelintrc \"src/**/*.{css,scss}\" --fix",
		"prepare": "husky install",
		"generate-pwa-assets": "pwa-assets-generator",
		"serve-json": "json-server --watch db.json --port 4000"
	},
	"dependencies": {
		"@antfu/eslint-config": "^3.8.0",
		"@sveltejs/kit": "^1.26.0",
		"@unocss/extractor-svelte": "^0.63.6",
		"@unocss/reset": "^0.63.6",
		"gsap": "3.12.5",
		"husky": "^9.1.6",
		"json-server": "^0.17.4",
		"svelte-i18n": "^4.0.1",
		"vanilla-tilt": "^1.8.1"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/adapter-static": "^3.0.6",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@sveltejs/vite-plugin-svelte-inspector": "^3.0.1",
		"@testing-library/jest-dom": "^6.1.4",
		"@tsconfig/svelte": "^5.0.4",
		"@typescript-eslint/eslint-plugin": "^8.12.2",
		"@typescript-eslint/parser": "^8.12.2",
		"@unocss/core": "^0.63.6",
		"@unocss/eslint-config": "^0.63.6",
		"@unocss/eslint-plugin": "^0.63.6",
		"@unocss/extractor-pug": "^0.63.6",
		"@unocss/preset-icons": "^0.63.6",
		"@unocss/preset-uno": "^0.63.6",
		"@unocss/svelte-scoped": "^0.63.6",
		"@unocss/transformer-directives": "^0.63.6",
		"@unocss/transformer-variant-group": "^0.63.6",
		"autoprefixer": "^10.4.16",
		"eslint": "^9.14.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.34.0",
		"jsdom": "^22.1.0",
		"postcss": "^8.4.31",
		"prettier": "^3.0.3",
		"prettier-plugin-css-order": "^2.0.1",
		"prettier-plugin-svelte": "^3.0.3",
		"sass": "^1.69.3",
		"sass-loader": "^13.3.2",
		"svelte": "^5.1.9",
		"svelte-check": "^4.0.5",
		"svelte-preprocess": "^6.0.3",
		"sveltekit-autoimport": "^1.7.1",
		"tslib": "^2.6.2",
		"typed.js": "^2.0.12",
		"typescript": "^5.2.2",
		"unocss": "^0.63.6",
		"vite": "^5.4.10",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-eruda": "^1.0.1",
		"vite-plugin-svg-icons": "^2.0.1"
	}
}
