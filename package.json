{
  "name": "pd3-vault-cracker",
  "private": true,
  "version": "0.1.3",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "dev:remote": "vite --host --open",
    "build": "pwa-assets-generator && vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tscon fig.json",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui --open",
    "test:watch": "vitest",
    "lint": "eslint -c .eslintrc --ext .svelte,.ts src/",
    "lint:styles": "stylelint --config .stylelintrc \"src/**/*.{css,scss}\"",
    "format": "eslint -c .eslintrc --fix --ext=.svelte,.ts --fix src",
    "format:prettier": "prettier --write src/**/{*.svelte,*.ts,*.scss}",
    "format:stylelint": "stylelint --config .stylelintrc \"src/**/*.{css,scss}\" --fix",
    "prepare": "husky install",
    "generate-pwa-assets": "pwa-assets-generator",
    "serve-json": "json-server --watch db.json --port 4000"
  },
  "devDependencies": {
    "@picocss/pico": "^2.0.6",
    "@rollup/plugin-alias": "^5.0.1",
    "@sveltejs/adapter-auto": "^3.2.0",
    "@sveltejs/kit": "^1.26.0",
    "@sveltejs/package": "^2.2.2",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/svelte": "^4.1.0",
    "@tsconfig/svelte": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@unocss/core": "latest",
    "@unocss/eslint-config": "^0.59.0",
    "@unocss/extractor-pug": "^0.59.0",
    "@unocss/preset-icons": "latest",
    "@unocss/preset-uno": "^0.59.0",
    "@unocss/svelte-scoped": "^0.57.1",
    "@unocss/transformer-directives": "^0.57.1",
    "@vite-pwa/assets-generator": "^0.2.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.31",
    "prettier": "^3.2.5",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-svelte": "^3.2.2",
    "sass": "^1.74.1",
    "sass-loader": "^13.3.2",
    "stylelint": "^16.3.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended": "^12.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^5.0.0",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.9",
    "svelte-i18n": "^4.0.0",
    "svelte-preprocess": "^5.0.4",
    "sveltekit-autoimport": "^1.8.0",
    "tslib": "^2.6.2",
    "typed.js": "^2.0.12",
    "typescript": "^5.4.4",
    "unocss": "^0.57.1",
    "vite": "^5.2.8",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eruda": "^1.0.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-mock": "^3.0.1",
    "vite-plugin-pages": "^0.32.1",
    "vite-plugin-progress": "^0.0.7",
    "vite-plugin-pwa": "^0.19.8",
    "vite-plugin-restart": "^0.4.0",
    "vite-plugin-svg-icons": "^2.0.1"
  },
  "dependencies": {
    "@antfu/eslint-config": "^1.1.0",
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@sveltejs/kit": "^1.26.0",
    "@types/cookie": "^0.5.3",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "cookie": "^0.5.0",
    "gsap": "3.8.0",
    "json-server": "^0.17.4",
    "node-sass": "^9.0.0",
    "svelte-spa-router": "^3.3.0",
    "vanilla-tilt": "^1.8.1"
  }
}
